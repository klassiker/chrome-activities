{
  "name": "Activity Manager",
  "version": "1.0",
  "description": "Manage activites by saving a list of open tabs",
  "permissions": ["tabs", "storage", "declarativeContent"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
